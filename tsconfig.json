{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "types": [],
    "baseUrl": ".",
    "paths": {
      "@modules/*": [
        "src/modules/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@content/*": [
        "src/content/*"
      ],
      "@scripts/*": [
        "src/scripts/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
    }
  },
}
